{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Train PD Gait",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/train.py",
			"args": ["--config", "configs/default.yaml"],
			"console": "integratedTerminal"
		},
		{
			"name": "Convert PD3D",
			"type": "python",
			"request": "launch",
			"module": "pd_gait.datasets.pd3d_convert",
			"args": ["--raw_dir", "data/raw/pd3d", "--data_dir", "data", "--views", "8"],
			"console": "integratedTerminal"
		}
	]
}
